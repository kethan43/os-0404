#include <stdio.h>

int main() {
    int b[10], p[10], nb, np, i, j, w;

    scanf("%d", &nb);
    for(i=0;i<nb;i++) scanf("%d",&b[i]);

    scanf("%d", &np);
    for(i=0;i<np;i++) scanf("%d",&p[i]);

    for(i=0;i<np;i++){
        w=-1;
        for(j=0;j<nb;j++)
            if(b[j]>=p[i] && (w==-1 || b[j]>b[w]))
                w=j;
        if(w!=-1){
            printf("Process %d allocated to block %d\n",i,w);
            b[w]-=p[i];
        }
    }
}
